# Database
DATABASE_URL=postgresql://user:password@localhost:5432/market_research

# Redis
REDIS_URL=redis://localhost:6379/0

# Reddit API (Get from: https://www.reddit.com/prefs/apps)
REDDIT_CLIENT_ID=your_client_id_here
REDDIT_CLIENT_SECRET=your_client_secret_here
REDDIT_USER_AGENT=MarketResearchBot/1.0

# Apify (Get from: https://apify.com - Settings > Integrations > API tokens)
APIFY_API_TOKEN=your_apify_token_here

# Cloudflare AI (Get from: https://dash.cloudflare.com/profile/api-tokens)
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# Claude API (Get from: https://console.anthropic.com)
ANTHROPIC_API_KEY=your_anthropic_key_here

# OpenAI (Get from: https://platform.openai.com/api-keys)
# OPENAI_API_KEY=your_openai_key_here

# App Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
